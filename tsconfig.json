{
  "compilerOptions": {
	"target": "es5",
	"module": "ESNext",
	"lib": [
	  "dom",
	  "dom.iterable",
	  "esnext"
	],
	"allowJs": true,
	"jsx": "react",
	"sourceMap": true,
	"noEmit": true,
	"strict": true,
	"noImplicitAny": false,
	"strictNullChecks": false,
	"noFallthroughCasesInSwitch": true,
	"moduleResolution": "node",
	"esModuleInterop": true,
	"skipLibCheck": true,
	"forceConsistentCasingInFileNames": true
  },
  "include": [
	"src/**/*"
  ]
}
